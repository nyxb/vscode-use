{
  "publisher": "nyxb",
  "name": "vscode-use",
  "displayName": "Vscode Use",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.2",
  "description": "",
  "author": "Dennis Ollhoff <contact@nyxb.xyz>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nyxb",
  "homepage": "https://ðŸ’»nyxb.ws",
  "repository": {
    "type": "git",
    "url": "https://github.com/nyxb/vscode-use"
  },
  "bugs": {
    "url": "https://github.com/nyxb/vscode-use/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/nyxb"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.79.2"
  },
  "contributes": {
    "commands": [
      {
        "command": "use.insertUseClientStatement",
        "title": "Insert 'use client' Statement"
      },
      {
        "command": "use.insertUseServerStatement",
        "title": "Insert 'use server' Statement"
      }
    ],
    "keybindings": [
      {
        "command": "use.insertUseClientStatement",
        "key": "ctrl+alt+c",
        "mac": "ctrl+alt+c",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "command": "use.insertUseServerStatement",
        "key": "ctrl+alt+s",
        "mac": "ctrl+alt+s",
        "when": "editorTextFocus && !editorReadonly"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nyxr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nyxr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "nyxlx changelogen@latest --release && nyxr publish"
  },
  "devDependencies": {
    "@nyxb/eslint-config": "^0.0.53",
    "@nyxb/nyxi": "^0.0.30",
    "@types/node": "^20.3.1",
    "@types/vscode": "^1.79.1",
    "@vscode/vsce": "^2.19.0",
    "dynot": "^0.1.8",
    "eslint": "^8.43.0",
    "kolibry": "^0.0.2",
    "pnpm": "^8.6.2",
    "rimraf": "^5.0.1",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vitest": "^0.31.4"
  }
}
